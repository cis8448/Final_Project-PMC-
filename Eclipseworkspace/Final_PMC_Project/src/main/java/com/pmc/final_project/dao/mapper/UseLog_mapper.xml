<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pmc.final_project.dao.IUseLogDao">
	<select id="getmemberuseLog"  parameterType="String" resultType="UseLogBean" >
		SELECT * FROM USELOG WHERE U_M_ID=#{m_id} 		
	</select>
	
	<select id="getUseLogCount" resultType="int" parameterType="String">
		SELECT COUNT(U_M_ID) FROM USELOG WHERE U_M_ID=#{m_id}
	</select>

</mapper>